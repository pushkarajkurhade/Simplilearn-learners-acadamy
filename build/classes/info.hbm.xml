<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>

	<class name="com.simplilearn.com.Info" table="sinfo">
		    <id name="ID">
            <generator class="identity"></generator>
        </id>

		
		<property name="studentName" type="string"></property>
		<property name="stPhone" type="string"></property>
		<property name="stAddress" type="string"></property>
		<property name="courseName" type="string"></property>
		
		
		
	</class>
	
	
</hibernate-mapping>